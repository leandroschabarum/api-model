{
    "name": "leandro/api-model",
    "description": "Extendable class for REST API based models - instead of the traditional Database based one - that integrates common Laravel's Model class functionalities.",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Leandro\\ApiModel\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Leandro Schabarum",
            "email": "leandroschabarum.98@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Leandro\\ApiModel\\Providers\\ApiModelServiceProvider"
            ]
        }
    },
}
